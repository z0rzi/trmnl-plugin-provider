{
  "enabled": true,
  "refreshRateMinutes": 1,

  "config": {
    "googleCalendar": {
      "serviceAccountKeyPath": "/path/to/your/service-account-key.json",
      "calendarIds": [
        "your.calendar@gmail.com",
